<Application
    x:Class="GContacts.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:GContacts" RequestedTheme="Light">

    <Application.Resources>
        <SolidColorBrush x:Name="GoogleBlue" Color="#0266C8" />
        <SolidColorBrush x:Name="GoogleRed" Color="#F90101" />
        <SolidColorBrush x:Name="GoogleGreen" Color="#00933B" />
        <SolidColorBrush x:Name="GoogleYellow" Color="#F2B50F" />
        <SolidColorBrush x:Name="GoogleBlueDark" Color="#0266C8" />
        <SolidColorBrush x:Name="GoogleGray" Color="#666" />
        <SolidColorBrush x:Name="GoogleDarkGray" Color="#424242" />
    </Application.Resources>

</Application>
